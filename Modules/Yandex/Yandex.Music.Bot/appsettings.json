{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] [v{SourceSystemInformationalVersion}] {Message}{NewLine}{Exception}",
          "pathFormat": "logs/lofi-yandex.music-{Date}.log",
          "retainedFileCountLimit": 5
        }
      },
      {
        "Name": "Console"
      }
    ]
  },
  "TelegramBot": {
    "Token": "<your token for telegram bot>"
  },
  "YandexMusic": {
    "Username": "<your yandex name>",
    "Password": "<your yandex password>"
  },
  "Proxy": {
    "Socks5Hostname": "<your proxy host name>",
    "Socks5Port": 1080,
    "Username": "<your proxy name>",
    "Password": "<your proxy password>"
  }
}